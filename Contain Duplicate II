class Solution {
public:
    bool containsDuplicate(vector<int>& nums) {
        
        
        for(auto i=nums.begin();i!=nums.end()-1;i++)
        {
            if(std::find (i+1, nums.end(), *i)!=nums.end())
            {
                return true;
            }
                
        }
        return false;
    }
};
